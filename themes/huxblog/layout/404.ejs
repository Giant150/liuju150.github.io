---
layout: layout
---

<!-- Page Header -->
<header class="intro-header"
    <% if (page["header-img"]){ %>
        style="background-image: url('<%= config.root %><%= page.path.replace('index.html', '') %><%= page["header-img"] %>')"
    <% } else { %>
        style="background-image: url('<%= config["header-img"] %>')"
    <% } %>
    >
    <div class="container">
        <div class="row">
            <div class="col-lg-8 col-lg-offset-2 col-md-10 col-md-offset-1 ">
                <div class="site-heading">
                    <h1>404</h1>
                    <!--<hr class="small">-->
                    <span class="subheading"><%= page.description || config.subtitle || "" %></span>
                </div>
            </div>
        </div>
    </div>
</header>

<script>
    document.body.classList.add('page-fullscreen');
</script>